programa {
  funcao inicio() {
  cadeia sexo
  escreva("digite f para feminino ou m para masculino: ")
  leia(sexo)

  se(sexo=='F' ou sexo=='f'){
    escreva("sexo feminino! ")
  }senao se(sexo == "M"ou sexo  == 'm'){
    escreva("sexo masculino")
  }
  senao{
    escreva("sexo invalido")
  }
  
  }
}
