programa {
  funcao inicio() {
    inteiro numero,quadrado,cubo

      escreva("digite um numero: ")
      leia(numero)

      quadrado = numero * numero
      
      cubo = numero * numero * numero

      escreva("o numero a o quadrado é: " , quadrado) 
      leia(quadrado)
      escreva("o numero a o cubo é: " , cubo)
      leia(cubo)

  }
}
